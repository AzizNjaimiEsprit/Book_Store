<AnchorPane prefHeight="529.0" prefWidth="796.0" stylesheets="@../Resources/css/Style2.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Views.Controllers.ChatViewAdmin">
   <children>
      <Accordion layoutX="330.0" layoutY="142.0" />
      <MenuBar layoutY="-2.0" prefHeight="25.0" prefWidth="796.0">
        <menus>
          <Menu mnemonicParsing="false" text="FAQ" onAction="#handlemenuclick">
            <items>
              <MenuItem id="ChatViewAdmin" mnemonicParsing="false" text="FAQ" />
                <MenuItem id="QuizViewAdmin" mnemonicParsing="false" text="Quiz" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <TableView fx:id="table_chat" layoutX="29.0" layoutY="142.0" prefHeight="328.0" prefWidth="738.0">
        <columns>
          <TableColumn fx:id="col_question" prefWidth="246.0" text="Questions" />
          <TableColumn fx:id="col_answer" prefWidth="351.0" text="Answers" />
            <TableColumn fx:id="col_update" prefWidth="134.0" />
        </columns>
      </TableView>
      <Button layoutX="320.0" layoutY="490.0" mnemonicParsing="false" onAction="#ajouterChatItem" prefHeight="25.0" prefWidth="156.0" styleClass="buttonBlue" text="Add FAQ">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Button>
      <Label layoutX="267.0" layoutY="77.0" styleClass="pagesTitles" text="Frequently Asked Questions">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <ImageView fitHeight="78.0" fitWidth="91.0" layoutX="29.0" layoutY="46.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Resources/images/faqIcon.jpg" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
